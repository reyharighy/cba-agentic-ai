# ---- Paths & Runtime ----
WORKDIR_PATH=/var/www/cba-agentic-ai
SQLITE_DB_PATH=/var/lib/sqlite-db

STREAMLIT_PORT=8501

# ---- PostgreSQL (External Business DB) ----
POSTGRES_PORT=5432
POSTGRES_USER=cba
POSTGRES_PASSWORD=password
POSTGRES_DB=postgres

# ---- pgAdmin (optional) ----
PGADMIN_PORT=5050
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=password

# ---- External DB Seeding ----
# Set to true to populate PostgreSQL with synthetic business data
ENABLE_EXTERNAL_DB_SEEDING=true

# --- Interactive Graph ---
# Set to true to show interactive graph
ENABLE_INTERACTIVE_GRAPH=true

# ---- LLM / Tooling APIs ----
GROQ_API_KEY=your_groq_api_key_here
E2B_API_KEY=your_e2b_api_key_here

# ---- Observability (Optional) ----
LANGSMITH_TRACING_V2=false
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_WORKSPACE_ID=your_workspace_id_here
